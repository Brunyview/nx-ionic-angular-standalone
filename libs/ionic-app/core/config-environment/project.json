{
  "projectType": "library",
  "sourceRoot": "libs/ionic-app/core/config-environment/src",
  "prefix": "my-org",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ionic-app/core/config-environment"],
